class Dog
{

String color="Red";
String name="muffin";
void sleeping()
{
  System.out.println("Muffin is sleeping");
}

}
public class Demo1
{
public static void main (String args[])
{
Dog d1;//Reference variable
d1= new Dog();//object creation
Dog d2= new Dog();
d1.color="Blue";
d1.name="Oreo";
d2.color="REd";
d2.name="Muffin";
System.out.println(d1.color);
System.out.println(d1.name);
System.out.println(d2.color);
System.out.println(d2.name);
d1.sleeping();
}
}
